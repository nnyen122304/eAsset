	<subDataset name="ContractAmountSum">
		<parameter name="applicationId" class="java.lang.String"/>
		<parameter name="tablePrefix" class="java.lang.String"/>
		<parameter name="versionWhere" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
  sum(a.ast_Get_Le_Month_Amount * a.quantity) * nagt.GET_LE_RE_COUNT totalContractAmount
FROM
   NEA_AP_GET_TAN_LINE_AST a
 , NEA_AP_GET_TAN nagt
where
 nagt.application_id = a.application_id(+)
and
  nagt.application_id = $P{applicationId}
group by nagt.GET_LE_RE_COUNT]]>
		</queryString>
		<field name="TOTALCONTRACTAMOUNT" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="applicationId" class="java.lang.String"/>
	<parameter name="versionWhere" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="tablePrefix" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="fromImage" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
  nagt.APPLICATION_ID applicationId
, nagt.GET_DELIVERY_DATE getDeliveryDate
, nagt.GET_LE_RE_COUNT getLeReCount
, nagt.GET_TOTAL_AMOUNT getTotalAmount
, NEA_UTIL_PKG.get_cn_value1_f('LEASE_RENTAL_CUSTOMER', nagt.GET_LE_RE_COMPANY_CODE, nagt.AP_COMPANY_CODE) getLeReCompanyName
, nagt.GET_LE_RE_ESTIMATE_NUMBER getLeReEstimateNumber
, NEA_UTIL_PKG.GET_CN_VALUE_F('REO_ORDER_SET', nagt.REO_ORDER_TYPE, 4, nagt.AP_COMPANY_CODE) reOrderTitle
, NEA_UTIL_PKG.get_cn_value_f('REO_ORDER_SET', nagt.REO_ORDER_TYPE, 5, nagt.AP_COMPANY_CODE) toSectionName
, NEA_UTIL_PKG.get_cn_value_f('REO_ORDER_SET', nagt.REO_ORDER_TYPE, 6, nagt.AP_COMPANY_CODE) fromSectionName
, nagt.REO_RECEPT_NUMBER reoReceptNumber
, nagt.REO_ORDER_DATE reoOrderDate
, nagt.REO_AST_STAFF_CODE reoAstStaffCode
, nagt.REO_AST_COMPANY_CODE reoAstCompanyCode
, NEA_UTIL_PKG.GET_COMPANY_OFFICIAL_NAME_F(nagt.REO_AST_COMPANY_CODE) reoAstCompanyOfficialName
, nagt.REO_AST_SECTION_CODE reoAstSectionCode
, nagt.REO_AST_SECTION_YEAR reoAstSectionYear
, nagt.REO_AST_TEL reoAstTel
, nagt.REO_AST_FAX reoAstFax
, nagt.REO_AST_MAIL_ADDRESS reoAstMailAddress
, nagt.REO_USE_HOPE_START_DATE reoUseHopeStartDate
, REO_INV_COMPANY_OFFICAIL_NAME reoInvCompanyOfficialName
, nagt.REO_INV_ADDRESS reoInvAddress
, nagt.REO_INV_TEL reoInvTel
, nagt.REO_INV_FAX reoInvFax
, nagt.REO_INV_MAIL_ADDRESS reoInvMailAddress
, nagt.REO_DLV_STAFF_CODE reoDlvStaffCode
, nagt.REO_DLV_COMPANY_CODE reoDlvCompanyCode
, NEA_UTIL_PKG.GET_COMPANY_OFFICIAL_NAME_F(nagt.REO_DLV_COMPANY_CODE) reoDlvCompanyOfficialName
, nagt.REO_DLV_SECTION_CODE reoDlvSectionCode
, nagt.REO_DLV_SECTION_YEAR reoDlvSectionYear
, nagt.REO_DLV_ADDRESS reoDlvAddress
, nagt.REO_DLV_TEL reoDlvTel
, nagt.REO_DLV_FAX reoDlvFax
, nagt.REO_DLV_MAIL_ADDRESS reoDlvMailAddress
, nagt.REO_DESCRIPTION reoDescription
, nagt.REO_INV_STAFF_INPUT_TYPE reoInvStaffInputType
, nagt.REO_INV_STAFF_NAME reoInvStaffName
, nagt.REO_INV_COMPANY_NAME reoInvCompanyName
, nagt.REO_INV_SECTION_NAME reoInvSectionName
, NEA_UTIL_PKG.get_staff_name_f(nagt.REO_AST_STAFF_CODE) reoAstStaffName
, NEA_UTIL_PKG.get_staff_name_f(nagt.REO_DLV_STAFF_CODE) reoDlvStaffName
, NEA_UTIL_PKG.get_company_name_f(nagt.REO_AST_COMPANY_CODE) reoAstCompanyName
, NEA_UTIL_PKG.get_company_name_f(nagt.REO_DLV_COMPANY_CODE) reoDlvCompanyName
, NEA_UTIL_PKG.get_section_name_f(nagt.REO_AST_COMPANY_CODE, nagt.REO_AST_SECTION_CODE, nagt.REO_AST_SECTION_YEAR) reoAstSectionName
, NEA_UTIL_PKG.get_section_name_f(nagt.REO_DLV_COMPANY_CODE, nagt.REO_DLV_SECTION_CODE, nagt.REO_DLV_SECTION_YEAR) reoDlvSectionName
, nagtl.*
, nagtl.astGetLeMonthAmount * quantity mounthAmount
, (nagtl.astGetLeMonthAmount * quantity) * nagt.GET_LE_RE_COUNT contractAmount
, nagt.GET_LE_RE_TOTAL_AMOUNT getLeReTotalAmount
, (SELECT SUM(NVL(c.GET_OTR_AMOUNT, 0)) FROM NEA_AP_GET_TAN_LINE_OTR_COST c WHERE c.APPLICATION_ID = nagt.APPLICATION_ID  ) totalCostAmount
, NEA_UTIL_PKG.GET_CN_VALUE_F('REO_ORDER_SET', nagt.REO_ORDER_TYPE, 7, nagt.AP_COMPANY_CODE) quoteFlag
, (SELECT REO_AGREEMENT FROM NEA_AP_GET_TAN_LINE_AST a WHERE a.APPLICATION_ID = nagt.APPLICATION_ID AND LINE_SEQ = 1 ) reoAgreement
, (SELECT REO_PRICE_LIST FROM NEA_AP_GET_TAN_LINE_AST a WHERE a.APPLICATION_ID = nagt.APPLICATION_ID AND LINE_SEQ = 1 ) reoPriceList
, nagt.REO_AST_OIR reoAstOir
FROM
  NEA_$P!{tablePrefix}AP_GET_TAN nagt
  , (
    SELECT
      APPLICATION_ID applicationId
    , '1' LINE_TYPE
    , LINE_SEQ lineSeq
    , AST_MODEL_CODE || ' ' || REO_AST_NAME || ' ' || AST_MAKER_MODEL astName
    , QUANTITY quantity
    , AST_MAKER_NAME astMakerName
    , AST_GET_LE_MONTH_AMOUNT astGetLeMonthAmount
    , TO_NUMBER(NULL) getOtrAmount
    , TO_CHAR(NULL) getOtrComment
    FROM
      NEA_$P!{tablePrefix}AP_GET_TAN_LINE_AST
    WHERE
      1 = 1
    $P!{versionWhere}
    UNION ALL
    SELECT
      APPLICATION_ID
    , '2' LINE_TYPE
    , LINE_SEQ
    , TO_CHAR(NULL)
    , TO_NUMBER(NULL)
    , TO_CHAR(NULL)
    , TO_NUMBER(NULL)
    , GET_OTR_AMOUNT
    , GET_OTR_COMMENT
    FROM
      NEA_$P!{tablePrefix}AP_GET_TAN_LINE_OTR_COST
    WHERE
      1 = 1
    $P!{versionWhere}
  ) nagtl
WHERE
  nagt.application_id = $P{applicationId}
    $P!{versionWhere}
AND nagt.APPLICATION_ID = nagtl.applicationId(+)]]>
	</queryString>
