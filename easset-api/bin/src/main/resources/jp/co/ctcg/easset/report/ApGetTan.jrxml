  , NULL estimateNumber
  , GET_LE_RE_COMMENT lineComment
FROM
  NEA_$P!{tablePrefix}AP_GET_TAN
WHERE
  APPLICATION_ID = $P{applicationId}
  AND GET_LE_RE_COMMENT IS NOT NULL
  $P!{versionWhere}
ORDER BY
