		<queryString>
			<![CDATA[SELECT
  '1' commentType
  , '購入先' commentTypeName
  , LINE_SEQ lineSeq
  , GET_PUR_COMPANY_NAME companyName
  , GET_PUR_ESTIMATE_NUMBER estimateNumber
  , GET_PUR_COMMENT lineComment
FROM
  NEA_$P!{tablePrefix}AP_GET_TAN_LINE_PUR
