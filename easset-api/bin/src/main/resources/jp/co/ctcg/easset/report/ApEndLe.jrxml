		</parameter>
		<queryString>
			<![CDATA[SELECT
      SUM(RE_AMOUNT) reAmount
    , SUM(PURCHASE_AMOUNT) purchaseAmount
FROM
    (
SELECT
    0 AS RE_AMOUNT
    , 0 AS PURCHASE_AMOUNT
FROM
    DUAL
UNION ALL
SELECT
    NVL(RE_AMOUNT, 0), 0
FROM
    NEA_AP_END_LE_LINE_LLD
WHERE
    (AP_END_LE_LINE_TYPE = '2' OR AP_END_LE_LINE_TYPE IS NULL)
    AND APPLICATION_ID = $P{applicationIdWhere}
UNION ALL
SELECT
    0, NVL(PURCHASE_AMOUNT, 0)
FROM
    NEA_AP_END_LE_LINE_LLD
WHERE
    (AP_END_LE_LINE_TYPE = '3' OR AP_END_LE_LINE_TYPE IS NULL)
    AND APPLICATION_ID = $P{applicationIdWhere}
) lld]]>
