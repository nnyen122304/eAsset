	<queryString>
		<![CDATA[SELECT
    nil.COMPANY_CODE
    , NEA_UTIL_PKG.get_company_name_f(nil.COMPANY_CODE) companyName
    ,nil.HOL_SECTION_CODE
    , NEA_UTIL_PKG.get_section_name_f(nil.COMPANY_CODE, nil.HOL_SECTION_CODE, nil.HOL_SECTION_YEAR) holSectionName
    ,nil.HOL_SECTION_YEAR
    ,nil.CREATE_DATE
    ,nil.UPDATE_DATE
    ,nil.PERIOD
    , NEA_UTIL_PKG.format_date_char_f(nil.PERIOD) periodF
    ,nil.INV_ASSET_TYPE
    ,nil.INV_STATUS
    ,nil.INV_COMMENT
    ,nil.AST_LIC_ID
    ,nil.PP_ID
    ,nil.AST_NUM
    ,nil.CONTRACT_NUM
    ,nil.CONTRACT_SUB_NUM
    ,nil.APPLICATION_ID
    ,nil.DELETE_FLAG
    ,nil.DELETE_DATE
    ,nil.AST_NAME
    ,nil.HOL_STAFF_CODE
    , NEA_UTIL_PKG.get_staff_name_f(nil.HOL_STAFF_CODE) holStaffName
    ,nil.USE_STAFF_CODE
    , NEA_UTIL_PKG.get_staff_name_f(nil.USE_STAFF_CODE) useStaffName
    ,nil.HOL_OFFICE_CODE
    , NEA_UTIL_PKG.get_cn_value1_f('OFFICE', nil.HOL_OFFICE_CODE) holOfficeName
    ,nil.HOL_OFFICE_FLOOR
    ,nil.HOL_OFFICE_ROOM_NUM
    ,nil.HOL_OFFICE_RACK_NUM
    ,nil.HOL_QUANTITY
    ,nil.DSC_DESCRIPTION
    ,nisum.AP_STATUS
    , NEA_UTIL_PKG.get_cn_value1_f('AP_STATUS_INV', nisum.AP_STATUS) apStatusName
    ,nisum.AP_DATE
    ,nisum.APPROVE_DATE
    ,ptvla.KYKNO
    ,ptvla.NINI_LD_17NM
    ,ptvla.SHISANNM
        ,ptvla.SETCHICD
        ,ptvla.SETCHINM
    ,ptvlc.LAKIKAN
    ,ptvlc.LATORIHIKIKBN
    , NEA_UTIL_PKG.get_cn_value1_f('PPFS_LLD_LATORIHIKIKBN', ptvlc.LATORIHIKIKBN) LATORIHIKIKBNNM
    ,ptvlc.BSKEIJOKBN
    , NEA_UTIL_PKG.get_cn_value1_f('PPFS_LLD_BSKEIJOKBN', ptvlc.BSKEIJOKBN) BSKEIJOKBNNM
    ,ptvlc.LAKAISU
    ,ptvla.LARYOHFCD
    ,ptvla.LARYOHFNM
    ,ptvlc.LAKAISHACD
    ,ptvlc.TORIHIKINM LAKAISHANM
    , NEA_UTIL_PKG.format_date_char_f(ptvlc.LASTYMD) STYMDF
    , NEA_UTIL_PKG.format_date_char_f(ptvlc.LAMANRYOYMD) LAMANRYOYMDF
    ,ptvla.nini_Ld_7cd COSTKBNCD
        ,ptvla.nini_Ld_7nm COSTKBNNM
    ,ptvla.nini_Ld_6cd PRJCD
       ,ptvla.nini_Ld_6nm PRJNM
    ,ptvla.SOSHIKI2CD
    ,ptvla.SOSHIKI2NM
    ,ptvla.BIKO1
    ,ptvla.BIKO2
    ,pf.STKGKK stkgkk
    ,pf.TO_BOKAK
    ,ptvla.LARYO_TOTAL
    ,pla.IKKAILARYO
    ,stat.LAST_SUCCESS_CREATE_END_DATE
    ,CASE
        WHEN nil.INV_STATUS IN('1','2','3')THEN '1'
        ELSE '2'
    END invStatusSort

FROM
    NEA_$P!{histTablePrefixLdInv}INV_LINE nil
    ,NEA_$P!{histTablePrefixLdInv}INV_SUM nisum
    ,NEA_$P!{histTablePrefixLdLld}PPFS_LLDC ptvlc
    ,NEA_$P!{histTablePrefixLdLld}PPFS_LLDA ptvla
    ,NEA_$P!{histTablePrefixLdLld}PPFS_RELATION_LA pzvr
    ,NEA_$P!{histTablePrefixLdLld}PPFS_LD_ASSETS pla
    ,NEA_$P!{histTablePrefixLdLld}PPFS_FLD pf
    ,NEA_INV_STAT stat

WHERE
     $P!{holSectionWhere}
    $P!{invStaffWhere}
    and nil.PERIOD = $P{period}
    and nil.COMPANY_CODE = $P{companyCode}
    and nil.INV_ASSET_TYPE = '5'
    and nil.INV_LINK_TYPE = '1'

    -- 棚卸明細と棚卸集約の結合
    and nil.PERIOD = nisum.PERIOD
    and nil.COMPANY_CODE = nisum.COMPANY_CODE
    and nil.INV_ASSET_TYPE = nisum.INV_ASSET_TYPE
    and nil.HOL_SECTION_CODE = nisum.HOL_SECTION_CODE

    -- 棚卸明細と契約情報結合
    and nil.PERIOD = ptvla.PERIOD
    and nil.COMPANY_CODE = ptvla.COMPANY_CODE
    and nil.PP_ID = ptvla.KOYUNO

    -- リース契約・紐付
    and ptvlc.KOYUNO = pzvr.KOYUNO_KYK
    and ptvlc.PERIOD = pzvr.PERIOD
    and ptvlc.COMPANY_CODE = pzvr.COMPANY_CODE

    -- 物件契約・紐付
    and ptvla.KOYUNO = pzvr.KOYUNO_SHISAN
    and ptvla.PERIOD = pzvr.PERIOD
    and ptvla.COMPANY_CODE = pzvr.COMPANY_CODE

    -- 基本台帳(資産別)紐付
    and ptvla.KOYUNO = pla.KOYUNO
    and ptvla.PERIOD = pla.PERIOD
    and ptvla.COMPANY_CODE = pla.COMPANY_CODE

    -- 資産と物件紐付
    and ptvla.KOYUNO = pf.KOYUNO(+)
    and ptvla.PERIOD = pf.PERIOD(+)
    and ptvla.COMPANY_CODE = pf.COMPANY_CODE(+)

    -- 棚卸データ作成ステータスと棚卸明細紐付
    and stat.COMPANY_CODE = nil.COMPANY_CODE
    and stat.PERIOD = nil.PERIOD

    -- リース契約・物件紐付有効データのみ
    and pzvr.YUKOFLG = '1'
    and pzvr.CANCELSUMIFLG = '0'

ORDER BY
    nil.HOL_SECTION_CODE
    ,ptvla.SETCHICD
    ,nil.CONTRACT_NUM
    ,nil.CONTRACT_SUB_NUM
    ,nil.PP_ID
    ,invStatusSort
    ,nil.AST_LIC_ID]]>
	</queryString>
