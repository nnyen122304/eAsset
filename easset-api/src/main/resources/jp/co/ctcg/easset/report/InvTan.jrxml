	<queryString>
		<![CDATA[SELECT
	nil.COMPANY_CODE
	, NEA_UTIL_PKG.get_company_name_f(nil.COMPANY_CODE) companyName
	,nil.HOL_SECTION_CODE
	, NEA_UTIL_PKG.get_section_name_f(nil.COMPANY_CODE, nil.HOL_SECTION_CODE, nil.HOL_SECTION_YEAR) holSectionName
	,nil.HOL_SECTION_YEAR
	,nil.CREATE_DATE
	,nil.UPDATE_DATE
	,nil.PERIOD
	, NEA_UTIL_PKG.format_date_char_f(nil.PERIOD) periodF
	,nil.INV_ASSET_TYPE
	,nil.INV_STATUS
	,nil.INV_COMMENT
	,nil.AST_LIC_ID
	,nil.PP_ID
	,nil.AST_NUM
	,nil.APPLICATION_ID
	,nil.DELETE_FLAG
	,nil.DELETE_DATE
	,nil.AST_NAME
	,nil.HOL_STAFF_CODE
	, NEA_UTIL_PKG.get_staff_name_f(nil.HOL_STAFF_CODE) holStaffName
	,nil.USE_STAFF_CODE
	, NEA_UTIL_PKG.get_staff_name_f(nil.USE_STAFF_CODE) useStaffName
	,nil.HOL_OFFICE_CODE
	, NEA_UTIL_PKG.get_cn_value1_f('OFFICE', nil.HOL_OFFICE_CODE) holOfficeName
	,nil.HOL_OFFICE_FLOOR
	,nil.HOL_OFFICE_ROOM_NUM
	,nil.HOL_OFFICE_RACK_NUM
	,nil.HOL_QUANTITY
	,nil.DSC_DESCRIPTION
	,nisum.AP_STATUS
	, NEA_UTIL_PKG.get_cn_value1_f('AP_STATUS_INV', nisum.AP_STATUS) apStatusName
	,nisum.AP_DATE
	,nisum.APPROVE_DATE
	,ptvf.SHISANKNRKBN
	,ptvf.SETCHICD
	,ptvf.SETCHINM
	,ptvf.SHURUINM
	,ptvf.SKKHOK
	, NEA_UTIL_PKG.get_cn_value1_f('PPFS_FLD_SKKHO', ptvf.SKKHOK)  skkhokName
	, DECODE(ptvf.SKKHOK, '3', ptvf.SKKMMSUK, 'D', ptvf.SKKMMSUK, ptvf.TAINENK ) tainenk
	,ptvf.SKKHIHFCD
	,ptvf.HFNM
	,ptvf.STKYMD
	,NVL(NEA_UTIL_PKG.format_date_char_f(ptvf.STKYMD),NEA_UTIL_PKG.format_date_char_f(ptvf.CPKEIJOYMD)) stkymdF
	,ptvf.KADOYMD
	,NVL(NEA_UTIL_PKG.format_date_char_f(ptvf.KADOYMD),NEA_UTIL_PKG.format_date_char_f(ptvf.ADD_UP_PLAN_DATE)) kadoymdF
	,ptvf.SURYO
	,ptvf.COST_TYPE_NAME
	,ptvf.DEP_PRJ_CODE
	,ptvf.DEP_PRJ_NAME
	,ptvf.SOSHIKI2CD
	,ptvf.SOSHIKI2NM
	,ptvf.BIKO1
	,ptvf.BIKO2
	,ptvf.STKGKK
	,ptvf.TO_BOKAK
	,ptvf.SHISANNM
	,stat.LAST_SUCCESS_CREATE_END_DATE
	, CASE
		WHEN ptvf.SHISANKNRKBN = '1' THEN '非償却'
			WHEN ptvf.SKKHOK = '9' THEN '非償却'
		WHEN (ptvf.SKKHOK != '9' and SUBSTR(ptvf.KADOYMD, 1, 6) > ptvf.PERIOD) THEN '未償却'
		WHEN ptvf.SKKKANRYOFLGK = '1' THEN '償却完了'
		ELSE '償却中'
	END skkStatusName
	,CASE
		WHEN nil.INV_STATUS IN('1','2','3')THEN '1'
		ELSE '2'
	END invStatusSort

FROM
	NEA_$P!{histTablePrefixLdInv}INV_LINE nil
	,NEA_$P!{histTablePrefixLdInv}INV_SUM nisum
	,NEA_$P!{histTablePrefixLdFld}PPFS_FLD ptvf
	, NEA_INV_STAT stat

WHERE
	$P!{holSectionWhere}
	$P!{invStaffWhere}
	and nil.PERIOD = $P{period}
	and nil.COMPANY_CODE = $P{companyCode}
	and nil.INV_ASSET_TYPE = '1'
	and nil.INV_LINK_TYPE = '1'
	and nil.PERIOD = nisum.PERIOD
	and nil.COMPANY_CODE = nisum.COMPANY_CODE
	and nil.INV_ASSET_TYPE = nisum.INV_ASSET_TYPE
	and nil.HOL_SECTION_CODE = nisum.HOL_SECTION_CODE
	and nil.PERIOD = ptvf.PERIOD
	and nil.COMPANY_CODE = ptvf.COMPANY_CODE
	and nil.PP_ID = ptvf.KOYUNO
	and stat.COMPANY_CODE = nil.COMPANY_CODE
	and stat.PERIOD = nil.PERIOD

ORDER BY
	nil.HOL_SECTION_CODE
	,ptvf.SETCHICD
	,nil.AST_NUM
	,nil.PP_ID
	,invStatusSort
	,nil.AST_LIC_ID]]>
