{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      ["element", "attribute"],
      ["app", "ea"],
      "kebab-case"
    ],
    "max-line-length": [
      true,
      300
    ],
    "deprecation": false,
    "no-any": true,
    "no-string-literal": false
  }
}
